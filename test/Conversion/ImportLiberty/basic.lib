library(testlib) {
  time_unit : "1ns";

  cell(BUF) {
    area : 1.0;

    pin(A) {
      direction : input;
      capacitance : 0.002;
    }

    pin(Y) {
      direction : output;
      function : "A";

      timing() {
        related_pin : "A";
        timing_sense : positive_unate;

        cell_rise() {
          values : "0.012, 0.018";
        }

        cell_fall() {
          values : "0.020";
        }
      }
    }
  }
}
