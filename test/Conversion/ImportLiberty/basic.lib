// CHECK: hw.module @BUF(
// CHECK-SAME: synth.ccs.pilot.arcs = [#synth.ccs_pilot_arc<"A", "Y", [0.000000e+00, 5.000000e-01], [0.000000e+00, 1.000000e+00], [0.000000e+00, 5.000000e-01], [1.000000e+00, 0.000000e+00]>]
// CHECK-SAME: synth.ccs.pilot.receivers = [#synth.ccs_pilot_receiver<"A", "Y", [0.000000e+00, 1.000000e+00], [0.000000e+00, 1.000000e+00], [2.000000e-01, 3.000000e-01, 4.000000e-01, 5.000000e-01], [0.000000e+00, 1.000000e+00], [0.000000e+00, 1.000000e+00], [3.000000e-01, 4.000000e-01, 5.000000e-01, 6.000000e-01], [0.000000e+00, 1.000000e+00], [0.000000e+00, 1.000000e+00], [2.500000e-01, 3.500000e-01, 4.500000e-01, 5.500000e-01], [0.000000e+00, 1.000000e+00], [0.000000e+00, 1.000000e+00], [3.500000e-01, 4.500000e-01, 5.500000e-01, 6.500000e-01]>]
// CHECK-SAME: synth.nldm.arcs = [#synth.nldm_arc<"A", "Y", "positive_unate", [1.000000e-02, 3.000000e-02], [0.000000e+00, 2.000000e-01], [1.200000e-02, 1.800000e-02, 2.000000e-02, 3.000000e-02], [], [], [2.000000e-02], [1.000000e-02, 3.000000e-02], [0.000000e+00, 2.000000e-01], [8.000000e-03, 1.100000e-02, 1.300000e-02, 1.600000e-02], [], [], [1.250000e-01]>]

library(testlib) {
  time_unit : "1ns";

  cell(BUF) {
    area : 1.0;

    pin(A) {
      direction : input;
      capacitance : 0.002;
    }

    pin(Y) {
      direction : output;
      function : "A";

      timing() {
        related_pin : "A";
        timing_sense : positive_unate;

        cell_rise() {
          index_1 : "0.01, 0.03";
          index_2 : "0.0, 0.2";
          values : "0.012, 0.018, 0.020, 0.030";
        }

        cell_fall() {
          values : "0.020";
        }

        rise_transition() {
          index_1 : "0.01, 0.03";
          index_2 : "0.0, 0.2";
          values : "0.008, 0.011, 0.013, 0.016";
        }

        fall_transition() {
          values : "0.125";
        }

        output_current_rise() {
          index_1 : "0.0, 0.5";
          values : "0.0, 1.0";
        }

        output_current_fall() {
          index_1 : "0.0, 0.5";
          values : "1.0, 0.0";
        }

        receiver_capacitance1_rise() {
          index_1 : "0.0, 1.0";
          index_2 : "0.0, 1.0";
          values : "0.2, 0.3, 0.4, 0.5";
        }

        receiver_capacitance2_rise() {
          index_1 : "0.0, 1.0";
          index_2 : "0.0, 1.0";
          values : "0.3, 0.4, 0.5, 0.6";
        }

        receiver_capacitance1_fall() {
          index_1 : "0.0, 1.0";
          index_2 : "0.0, 1.0";
          values : "0.25, 0.35, 0.45, 0.55";
        }

        receiver_capacitance2_fall() {
          index_1 : "0.0, 1.0";
          index_2 : "0.0, 1.0";
          values : "0.35, 0.45, 0.55, 0.65";
        }
      }
    }
  }
}
